{
  "name": "national-grid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.11.1",
    "layui-layer": "^1.0.9",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "element-ui": "^2.11.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true,
      "amd": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console":2,
      "no-debugger":0,
      "no-alert": 0,
      "no-array-constructor": 2,
      "no-caller": 1,
      "no-cond-assign": 2,
      "no-class-assign": 2,
      "no-const-assign": 2,
      "no-dupe-keys": 2,
      "no-dupe-args": 2,
      "no-duplicate-case": 2,
      "no-else-return": 2,
      "no-empty": 2,
      "no-eval": 1,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-irregular-whitespace": 2,
      "no-lonely-if": 2,
      "no-mixed-spaces-and-tabs": [2, false],
      "no-multi-spaces": 1,
      "no-multi-str": 2,
      "no-multiple-empty-lines": [1, {"max": 2}],
      "no-nested-ternary": 2,
      "no-new-func": 1,
      "no-new-object": 2,
      "no-new-require": 2,
      "no-new-wrappers": 2,
      "no-param-reassign": 2,
      "no-plusplus": 0,
      "no-redeclare": 2,
      "no-return-assign": 1,
      "no-spaced-func": 2,
      "no-undef": 1,
      "no-unreachable": 2,
      "no-unused-expressions": 2,
      "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
      "no-use-before-define": 2,
      "no-var": 0,
      "no-with": 2,
      "camelcase": 2,
      "consistent-this": [2, "that"],
      "curly": [2, "all"],
      "default-case": 2,
      "eqeqeq": 2,
      "indent": [2, 4],
      "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
      "quote-props":[2, "as-needed"],
      "radix": 2,
      "semi": [2, "never"],
      "space-after-keywords": [0, "never"],
      "space-before-function-paren": [0, "never"],
      "space-in-parens": [0, "never"],
      "valid-jsdoc": 2,
      "arrow-parens":0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}